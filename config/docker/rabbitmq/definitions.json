{
  "rabbitmq_version": "3.12",
  "users": [
    {
      "name": "admin",
      "password_hash": "admin",
      "hashing_algorithm": "rabbit_password_hashing_sha256",
      "tags": "administrator"
    },
    {
      "name": "jobsprint",
      "password_hash": "jobsprint",
      "hashing_algorithm": "rabbit_password_hashing_sha256",
      "tags": "management"
    }
  ],
  "vhosts": [
    {
      "name": "/jobsprint"
    }
  ],
  "permissions": [
    {
      "user": "jobsprint",
      "vhost": "/jobsprint",
      "configure": ".*",
      "write": ".*",
      "read": ".*"
    },
    {
      "user": "admin",
      "vhost": "/jobsprint",
      "configure": ".*",
      "write": ".*",
      "read": ".*"
    }
  ],
  "queues": [
    {
      "name": "job_processing",
      "vhost": "/jobsprint",
      "durable": true,
      "type": "quorum",
      "auto_delete": false,
      "arguments": {
        "x-delivery-limit": 3,
        "x-quorum-initial-group-size": 3
      }
    },
    {
      "name": "webhook_events",
      "vhost": "/jobsprint",
      "durable": true,
      "type": "quorum",
      "auto_delete": false
    },
    {
      "name": "notifications",
      "vhost": "/jobsprint",
      "durable": true,
      "type": "quorum",
      "auto_delete": false
    },
    {
      "name": "workflow_execution",
      "vhost": "/jobsprint",
      "durable": true,
      "type": "quorum",
      "auto_delete": false
    }
  ],
  "exchanges": [
    {
      "name": "jobs",
      "vhost": "/jobsprint",
      "type": "topic",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    },
    {
      "name": "webhooks",
      "vhost": "/jobsprint",
      "type": "direct",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    },
    {
      "name": "notifications",
      "vhost": "/jobsprint",
      "type": "fanout",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    }
  ],
  "bindings": [
    {
      "source": "jobs",
      "vhost": "/jobsprint",
      "destination": "job_processing",
      "destination_type": "queue",
      "routing_key": "job.process",
      "arguments": {}
    },
    {
      "source": "jobs",
      "vhost": "/jobsprint",
      "destination": "job_processing",
      "destination_type": "queue",
      "routing_key": "job.retry",
      "arguments": {}
    },
    {
      "source": "webhooks",
      "vhost": "/jobsprint",
      "destination": "webhook_events",
      "destination_type": "queue",
      "routing_key": "webhook.send",
      "arguments": {}
    },
    {
      "source": "notifications",
      "vhost": "/jobsprint",
      "destination": "notifications",
      "destination_type": "queue",
      "routing_key": "",
      "arguments": {}
    },
    {
      "source": "jobs",
      "vhost": "/jobsprint",
      "destination": "workflow_execution",
      "destination_type": "queue",
      "routing_key": "workflow.execute",
      "arguments": {}
    }
  ],
  "policies": [
    {
      "name": "ha-all",
      "pattern": ".*",
      "vhost": "/jobsprint",
      "definition": {
        "ha-mode": "all",
        "ha-sync-mode": "automatic",
        "ha-sync-batch-size": 10
      }
    },
    {
      "name": "max-length",
      "pattern": ".*",
      "vhost": "/jobsprint",
      "definition": {
        "max-length": 10000,
        "overflow": "drop-head"
      }
    }
  ],
  "parameters": [],
  "global_parameters": [
    {
      "name": "cluster_name",
      "value": "jobsprint-cluster"
    }
  ]
}
