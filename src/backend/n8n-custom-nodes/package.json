{
  "name": "n8n-nodes-jobsprint",
  "version": "1.0.0",
  "description": "Jobsprint custom nodes for n8n - AI automation platform integration",
  "keywords": [
    "n8n",
    "n8n-community-nodes",
    "jobsprint",
    "ai",
    "automation",
    "puter",
    "zapier"
  ],
  "license": "MIT",
  "homepage": "https://github.com/jobsprint/n8n-nodes-jobsprint",
  "author": {
    "name": "Jobsprint",
    "email": "info@jobsprint.ai"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jobsprint/n8n-nodes-jobsprint.git"
  },
  "bugs": {
    "url": "https://github.com/jobsprint/n8n-nodes-jobsprint/issues"
  },
  "main": "index.js",
  "types": "index.d.ts",
  "n8n": {
    "n8nVersionCompat": ">=1.0.0",
    "credentials": [
      "dist/credentials/JobsprintApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/JobsprintAI.node.js",
      "dist/nodes/JobsprintPuterStorage.node.js",
      "dist/nodes/JobsprintZapier.node.js",
      "dist/nodes/JobsprintWorkflowTrigger.node.js",
      "dist/nodes/JobsprintWebhookHandler.node.js"
    ]
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials --ext .ts",
    "lint:fix": "eslint nodes credentials --ext .ts --fix",
    "prepublishOnly": "npm run build && npm run lint -s",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "n8n-workflow": "^1.65.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "eslint-plugin-n8n-nodes-base": "^1.13.1",
    "jest": "^29.7.0",
    "n8n": "^1.65.0",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.10",
    "npm": ">=9.0.0"
  },
  "files": [
    "dist",
    "credentials",
    "nodes"
  ]
}
